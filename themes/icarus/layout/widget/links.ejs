<% if (site.posts.length) { %>
    <div class="widget-wrap widget-list">
        <h3 class="widget-title"><%= __('widget.links') %></h3>
        <div class="widget">
            <ul>
                <% for (var i in theme.miscellaneous.links) { %>
                    <li>
                        <a href="<%- theme.miscellaneous.links[i] %>" target="_blank"><%= i %></a>
                    </li>
                <% } %>
            </ul>
        </div>
        <a href="https://heapanalytics.com/?utm_source=badge" target="_blank"><img style="width:108px;height:41px" src="//heapanalytics.com/img/badge.png" alt="Heap | Mobile and Web Analytics" /></a>
    </div>
<% } %>
